<% if (post.link){ %>
  <a class="<%= class_name %>" href="<%- url_for(post.link) %>" target="_blank" itemprop="url"><%= post.title %></a>
<% } else if (post.title){ %>
  <% if (index){ %>
    <a class="<%= class_name %>" href="<%- url_for(post.path) %>" itemprop="name"><%= post.title %></a>
  <% } else { %>
    <span itemprop="name"><%= post.title %></span>
  <% } %>
<% } %>